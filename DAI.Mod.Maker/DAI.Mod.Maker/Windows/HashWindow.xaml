<Window x:Class="DAI.Mod.Maker.HashWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="HashWindow" Height="300" Width="600" xmlns:modmaker="clr-namespace:DAI.Mod.Maker;assembly=DAI.Mod.Maker">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250*" />
            <ColumnDefinition Width="95*" />
            <ColumnDefinition Width="247*" />
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <TextBox Name="FNVSourceTextBox" Margin="10,10,10,0" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" />
            <CheckBox Name="FNVCaseSensitiveCB" Content="Case Sensitive?" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
        </StackPanel>
        <Button Name="FNVHashButton" Content="Hash" Grid.Column="1" Margin="10,10,10,0" VerticalAlignment="Top" Click="FNVHashButton_Click" MinWidth="50"/>
        <TextBox Name="FNVDestTextBox" Grid.Column="2" Margin="10,10,10,0" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" />
        
    </Grid>
</Window>