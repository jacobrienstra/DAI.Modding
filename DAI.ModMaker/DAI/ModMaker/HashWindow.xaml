<Window x:Class="DAI.ModMaker.HashWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="HashWindow" Height="300" Width="600" xmlns:modmaker="clr-namespace:DAI.ModMaker;assembly=DAI.ModMaker">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250*" />
            <ColumnDefinition Width="95*" />
            <ColumnDefinition Width="247*" />
        </Grid.ColumnDefinitions>
        <TextBox Name="FNVSourceTextBox" Height="23" Margin="10,10,10,0" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" />
        <Button Name="FNVHashButton" Content="Hash" Grid.Column="1" Margin="10,10,10,0" VerticalAlignment="Top" Height="23" Click="FNVHashButton_Click" />
        <TextBox Name="FNVDestTextBox" Grid.Column="2" Height="23" Margin="10,10,10,0" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" />
        <CheckBox Name="FNVCaseSensitiveCB" Content="Case Sensitive?" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" />
    </Grid>
</Window>