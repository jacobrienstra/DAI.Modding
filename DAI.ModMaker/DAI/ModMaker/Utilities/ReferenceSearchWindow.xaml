<Window x:Class="DAI.ModMaker.ReferenceSearchWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Find References" Height="398" Width="571" WindowStyle="ToolWindow" Background="#FFF0F0F0" xmlns:modmaker="clr-namespace:DAI.ModMaker;assembly=DAI.ModMaker">
    <Grid>
        <TextBox Name="SearchTextBox" Height="23" Margin="65,10,10,0" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" IsReadOnly="True" Background="#FFF0F0F0" />
        <TextBlock HorizontalAlignment="Left" Margin="10,14,0,0" TextWrapping="Wrap" Text="Asset:" VerticalAlignment="Top" Height="19" Width="50" />
        <ListBox Name="SearchResultsListBox" Margin="10,38" MouseDoubleClick="SearchResultsListBox_MouseDoubleClick">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Name="displayTextBox" Text="{Binding DisplayFullName}" />
                    </StackPanel>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ListBox>
        <Button Name="CloseButton" Content="Close" Margin="0,0,10,10" VerticalAlignment="Bottom" Height="23" HorizontalAlignment="Right" Width="79" Click="CloseButton_Click" />
        <TextBlock Name="SearchStatusText" Margin="10,0,94,5" TextWrapping="Wrap" Text="" VerticalAlignment="Bottom" Height="23" TextAlignment="Center" />
    </Grid>
</Window>